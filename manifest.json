  {
    "name": "Tapd - Any Status",
    "version": "0.1.0",
    "description": "Change tapd story and bug status at your will!",
    "author": "shawtung@qq.com",
    "manifest_version": 2,
    "icons": {
      "48": "icon_48.png",
      "128": "icon_128.png"
    },
    "content_scripts": [
      {
        "matches": ["https://www.tapd.cn/*", "http://www.tapd.cn/*"],
        "js": ["load.js"]
      }
    ],
    "permissions": ["activeTab"]
  }
